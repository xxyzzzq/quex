start = ONE;

token {
    LETTER;
}

mode BASE :
<skip: [ \n\t]> 
{
    <<EOF>> => QUEX_TKN_TERMINATION(LexemeNull);
}
mode ONE : BASE {
    2 => GOTO(TWO);
    3 => GOSUB(THREE, QUEX_TKN_LETTER(Lexeme));
    4 => GOTO(FOUR);
    [^ \n\t] => QUEX_TKN_LETTER(Lexeme);
    
}
mode TWO : BASE {
    1 => GOTO(ONE);
    3 => GOTO(THREE);
    4 => GOTO(FOUR);
    [^ \n\t] => QUEX_TKN_LETTER(Lexeme);
    
}
mode THREE : BASE {
    1 => GOTO(ONE);
    2 => GOTO(TWO);
    4 => GOTO(FOUR);
    [^ \n\t] => QUEX_TKN_LETTER(Lexeme);
    
}
mode FOUR : BASE {
    1 => GOTO(ONE);
    2 => GOTO(TWO);
    3 => GOTO(THREE);
    [^ \n\t] => QUEX_TKN_LETTER(Lexeme);
}
