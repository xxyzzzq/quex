start = ONE;

mode BASE :
<skip: [ \n\t]> 
{
    [.] => QUEX_TKN_LETTER(Lexeme);
    
}
mode ONE : BASE {
    2 => GOTO(TWO);
    3 => GOTO(THREE);
    4 => GOTO(FOUR);
}
mode TWO : BASE {
    1 => GOTO(ONE);
    3 => GOTO(THREE);
    4 => GOTO(FOUR);
}
mode THREE : BASE {
    1 => GOTO(ONE);
    2 => GOTO(TWO);
    4 => GOTO(FOUR);
}
mode FOUR : BASE {
    1 => GOTO(ONE);
    2 => GOTO(TWO);
    3 => GOTO(THREE);
}
