#endif

#ifndef    QUEX_TYPE_MODE
#   define QUEX_TYPE_MODE     QuexMode
#endif
#ifndef    QUEX_TYPE_MODE_TAG
#   define QUEX_TYPE_MODE_TAG QuexMode_tag
