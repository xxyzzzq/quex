QUEX_NAMESPACE_MAIN_OPEN

    // -- activate/deactivate byte order reversion (big-/little-endian)
    bool                  QUEX_FUNC(byte_order_reversion)(QUEX_TYPE_ANALYZER* me);
    void                  QUEX_FUNC(byte_order_reversion_set)(QUEX_TYPE_ANALYZER* me,
                                                              bool Value);
    // (*) Version information
    const char* QUEX_FUNC(version)(QUEX_TYPE_ANALYZER* me); // created by quex for version/date information

    // (*) Tell/Seek/Move on basis of character index
    void        QUEX_FUNC(move_forward)(QUEX_TYPE_ANALYZER*  me, const size_t);
    void        QUEX_FUNC(move_backward)(QUEX_TYPE_ANALYZER* me, const size_t);
    size_t      QUEX_FUNC(tell)(QUEX_TYPE_ANALYZER* me);
    void        QUEX_FUNC(seek)(QUEX_TYPE_ANALYZER* me, const size_t);

    void  QUEX_FUNC(print_this)(QUEX_TYPE_ANALYZER* me);

QUEX_NAMESPACE_MAIN_CLOSE
